<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.demo.persistence.LookupValuesDao">

    <select id="findByCode" resultType="com.example.demo.models.LookupValueDto">
        SELECT *
        FROM lookup_values
        WHERE code = #{code};
    </select>

</mapper>
